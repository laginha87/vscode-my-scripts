{
    "name": "my-scripts",
    "displayName": "my-scripts",
    "description": "",
    "version": "0.4.0",
    "publisher": "laginha87",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/laginha87/vscode-my-scripts.git"
    },
    "activationEvents": [
        "onCommand:myScripts.openScript",
        "onCommand:myScripts.openMyScripts"
    ],
    "license": "MIT",
    "extensionPack": [
        "formulahendry.code-runner"
    ],
    "icon": "assets/logo.png",
    "galleryBanner": {
        "color": "#9BBC0F",
        "theme": "dark"
    },
    "bugs": {
        "url": "https://github.com/laginha87/vscode-my-scripts/issues"
    },
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "myScripts.openScript",
                "title": "Open new script",
                "category": "My Scripts"
            },
            {
                "command": "myScripts.openMyScripts",
                "title": "Open my scripts",
                "category": "My Scripts"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^11.0.0",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vsce": "^1.62.0",
        "vscode": "^1.1.35"
    }
}
